# Cureon Environment Configuration
# Copy this file to .env and update with your values

# API Configuration
VITE_API_URL=https://your-backend-url.com
VITE_SOCKET_URL=https://your-backend-url.com

# For local development with mobile device
# Update with your computer's local IP address
# Find your IP: Windows (ipconfig) | Mac/Linux (ifconfig)
VITE_LOCAL_IP=192.168.1.100

# Stripe Payment
VITE_STRIPE_PUBLIC_KEY=your_stripe_public_key

# ZegoCloud Video Call
VITE_ZEGO_APP_ID=your_zego_app_id
VITE_ZEGO_SERVER_SECRET=your_zego_server_secret

# Google Maps (for location services)
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Cloudinary (for image uploads)
VITE_CLOUDINARY_CLOUD_NAME=your_cloudinary_cloud_name
VITE_CLOUDINARY_UPLOAD_PRESET=your_upload_preset

# App Configuration
VITE_APP_NAME=Cureon
VITE_APP_VERSION=1.0.0

# Environment
VITE_ENV=production
